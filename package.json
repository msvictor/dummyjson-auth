    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "lint:check": "tsc && eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "tsc && eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prettify:check": "prettier --config ./.prettierrc.js --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettify:write": "prettier --config ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
